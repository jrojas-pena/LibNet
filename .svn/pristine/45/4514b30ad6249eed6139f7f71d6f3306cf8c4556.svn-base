@import url('https://fonts.googleapis.com/css2?family=Poppins&display=swap');

.menu_area{
	background-color: #444;
	overflow: hidden;	
    font-family: 'Poppins', sans-serif;
}
.menu_area a {
  	float: left;
  	color: #f2f2f2;
  	text-align: center;
  	padding: 14px 16px;
  	text-decoration: none;
  	font-size: 17px;
  	display: block;
  	margin: 0;
}
.menu_area a:hover{
	background-color: #ffab00;
}
.menu_area h2{
	position: relative;
	left: 26%;
	color: #f2f2f2;
  	text-align: center;
  	text-decoration: none;
  	margin: 0;
  	margin-top: 5px;
  	display: inline-block;
}
#login-info{
	margin-top: 5px;
	padding: 0;
	display: inline-block;
	font-size: 12px;
	position: relative;
	left: 45%
}
ul{
	list-style-type: none;
	margin: 0;
}
li{
	display: inline;
}
#logout{
	float: right;
}