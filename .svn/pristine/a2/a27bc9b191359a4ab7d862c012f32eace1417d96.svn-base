<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LibNet</display-name>
  <error-page>
  	<exception-type>com.libnet.BadRequestException</exception-type>
  	<location>/badrequest.jspx</location>
  </error-page><filter>
    <display-name>RedirectFilter</display-name>
    <filter-name>RedirectFilter</filter-name>
    <filter-class>ca.on.senecac.prg556.common.RedirectFilter</filter-class>
    <init-param>
      <description></description>
      <param-name>url</param-name>
      <param-value>/showmaterialreservations.jspx</param-value>
    </init-param>
    <init-param>
      <description></description>
      <param-name>method</param-name>
      <param-value>REDIRECT</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>RedirectFilter</filter-name>
    <url-pattern>/</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>AccountAccessFilter</display-name>
    <filter-name>AccountAccessFilter</filter-name>
    <filter-class>com.libnet.AccountAccessFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AccountAccessFilter</filter-name>
    <url-pattern>*.jspx</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>ReserveMaterialsFilter </display-name>
    <filter-name>ReserveMaterialsFilter</filter-name>
    <filter-class>com.libnet.ReserveMaterialsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ReserveMaterialsFilter</filter-name>
    <url-pattern>/reservematerials.jspx</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>LimaraMenuFilter </display-name>
    <filter-name>LimaraMenuFilter</filter-name>
    <filter-class>com.libnet.LimaraMenuFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LimaraMenuFilter</filter-name>
    <url-pattern>/limaramenu.jspx</url-pattern>
    <dispatcher>INCLUDE</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>LibraryLoginFilter</display-name>
    <filter-name>LibraryLoginFilter</filter-name>
    <filter-class>com.libnet.LibraryLoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LibraryLoginFilter</filter-name>
    <url-pattern>/librarylogin.jspx</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>ShowMaterialReservationsFilter</display-name>
    <filter-name>ShowMaterialReservationsFilter</filter-name>
    <filter-class>com.libnet.ShowMaterialReservationsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ShowMaterialReservationsFilter</filter-name>
    <url-pattern>/showmaterialreservations.jspx</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <display-name>LogoutFilter</display-name>
    <filter-name>LogoutFilter</filter-name>
    <filter-class>com.libnet.LogoutFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LogoutFilter</filter-name>
    <url-pattern>/logout.jspx</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  
</web-app>